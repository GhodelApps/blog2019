{
  "name": "buildinfo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve-dist": "ws --hostname localhost -d dist/buildinfo -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --configuration production",
    "postbuild-prod": "node ./git-tag.js",
    "release-patch": "npm --no-git-tag-version version patch && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "12.2.12",
    "@angular/compiler": "12.2.12",
    "@angular/core": "12.2.12",
    "@angular/platform-browser": "12.2.12",
    "@angular/platform-browser-dynamic": "12.2.12",
    "@angular/router": "12.2.12",
    "rxjs": "7.4.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.12",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "12.2.12",
    "@angular/compiler-cli": "12.2.12",
    "@angular/language-service": "12.2.12",
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "local-web-server": "5.1.1",
    "replace-in-file": "6.3.2",
    "simple-git": "2.47.0",
    "typescript": "4.3.5"
  }
}