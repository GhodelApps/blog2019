{
  "name": "buildinfo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve-dist": "ws --hostname localhost -d dist/buildinfo -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod",
    "postbuild-prod": "node ./git-tag.js",
    "release-patch": "npm --no-git-tag-version version patch && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.7",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@angular/language-service": "11.2.8",
    "@types/node": "14.14.37",
    "codelyzer": "6.0.1",
    "local-web-server": "4.2.1",
    "replace-in-file": "6.2.0",
    "simple-git": "2.37.0",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  }
}
