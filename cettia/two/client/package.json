    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/router": "11.0.0",
    "@ionic/angular": "5.4.4",
    "cettia-client": "1.1.0",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "protobufjs": "6.10.1",
    "uuid": "8.3.1",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.1",
